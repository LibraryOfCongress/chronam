{% load custom_filters %}<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom"
      xmlns:opensearch="http://a9.com/-/spec/opensearch/1.1/">

    <title>Chronicling America Title Search Results</title>
    <link href="{{feed_url}}" rel="self" />
    <id>{{feed_url}}</id>
    <updated>{{updated}}</updated>
    <opensearch:totalResults>{{paginator.count}}</opensearch:totalResults>
    <opensearch:startIndex>{{page.start_index}}</opensearch:startIndex>
    <opensearch:itemsPerPage>50</opensearch:itemsPerPage>
    <link rel="search" type="application/opensearchdescription+xml" href="{% url 'openoni_search_pages_opensearch' %}" />
    {% if previous_url %}<link rel="previous" type="application/atom+xml" href="http://{{host}}{% url 'openoni_search_pages_results' %}{{ previous_url }}" />{% endif %}
    {% if next_url %}<link rel="next" type="application/atom+xml" href="http://{{host}}{% url 'openoni_search_pages_results' %}{{ next_url }}" />{% endif %}

    {% for title in page.object_list %}
    <entry>
        <title>{{title.display_name}}</title>
        <link href="http://{{host}}{{title.url}}" />
        <id>info:lccn/{{title.lccn}}</id>
        <author><name>Library of Congress</name></author>
        <updated>{{title.created|rfc3339}}</updated>
    </entry>
    {% endfor %}

</feed>

